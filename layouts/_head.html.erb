<link rel="alternate" href="http://yorickpeterse.com/articles/atom"
type="application/atom+xml" title="<%= config[:title] %>" />

<link rel="canonical" href="<%= @canonical ? @canonical : config[:base_url] %>" />

<% if @keywords %>
<meta name="keywords" content="<%= @keywords.join(',') %>" />
