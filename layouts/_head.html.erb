<link rel="stylesheet" href="/css/minified.css" />

<% else %>

<link rel="stylesheet" href="/css/reset.css" />
<link rel="stylesheet" href="/css/github.css" />
<link rel="stylesheet" href="/css/style.css" />

<% end %>

